{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "disabled_hooks": [
    "non-interactive-env",
    "todo-continuation-enforcer"
  ],
  "agents": {
    "librarian": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "explore": {
      "model": "anthropic/claude-haiku-4-5"
    },
    "frontend-ui-ux-engineer": {
      "model": "anthropic/claude-opus-4-6"
    },
    "document-writer": {
      "model": "anthropic/claude-opus-4-6"
    },
    "multimodal-looker": {
      "model": "anthropic/claude-haiku-4-5"
    },
    "sisyphus": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "oracle": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "prometheus": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "metis": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "momus": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "atlas": {
      "model": "anthropic/claude-sonnet-4-5"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "ultrabrain": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "quick": {
      "model": "anthropic/claude-haiku-4-5"
    },
    "unspecified-low": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "unspecified-high": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "medium"
    },
    "writing": {
      "model": "anthropic/claude-sonnet-4-5"
    }
  }
}
